<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace></RootNamespace>
    <NoWarn>CS0649;CS0169</NoWarn>
    <NukeRootDirectory>..</NukeRootDirectory>
    <NukeScriptDirectory>..</NukeScriptDirectory>
    <NukeTelemetryVersion>1</NukeTelemetryVersion>
    <Configurations>Debug;Release;Pack</Configurations>
  </PropertyGroup>

  <ItemGroup>
    <PackageDownload Include="NuGet.CommandLine" Version="[5.10]" />
    <PackageReference Include="GitVersion.CommandLine" Version="5.7.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Nuke.Common" Version="5.3.0" />
    <PackageReference Include="Nuke.GitHub" Version="2.0.0" />
    <PackageDownload Include="docfx.console" Version="[2.56.6]" />
    <PackageDownload Include="GitVersion.Tool" Version="[5.5.1]" />
  </ItemGroup>

</Project>
